import keyboard
import time


a= 1
b= 2
c= 5
while True:
    if keyboard.is_pressed("t"):
        try:
            c= int(input("Nyt tal"))
        except ValueError:
            print("ugyldigt")
    if c == 5:
        print(a)
        time.sleep(1)
    else:
        print(b)
        time.sleep(1)
